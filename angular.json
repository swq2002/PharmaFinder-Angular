{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "PharmaFinder": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pharma-finder",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",

              "src/assets/AdminAssets/images/favicon.png" 


            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "src/assets/HomeAssets/css/font-icons.css",
              "src/assets/HomeAssets/css/plugins.css",
              "src/assets/HomeAssets/css/style.css",
              "src/assets/HomeAssets/css/responsive.css",


              "src/assets/AdminAssets/vendors/mdi/css/materialdesignicons.min.css",
              "src/assets/AdminAssets/vendors/flag-icon-css/css/flag-icon.min.css",
              "src/assets/AdminAssets/vendors/css/vendor.bundle.base.css",
              "src/assets/AdminAssets/vendors/font-awesome/css/font-awesome.min.css",
              "src/assets/AdminAssets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.css"
              ,
              "src/assets/AdminAssets/css/style.css"





        
                 
              ],
            "scripts": [
              
              "src/assets/HomeAssets/js/plugins.js"
            ,"src/assets/HomeAssets/js/main.js",

            "src/assets/AdminAssets/vendors/js/vendor.bundle.base.js",
            "src/assets/AdminAssets/vendors/chart.js/Chart.min.js",
            "src/assets/AdminAssets/vendors/jquery-circle-progress/js/circle-progress.min.js",
            "src/assets/AdminAssets/js/off-canvas.js",
            "src/assets/AdminAssets/js/hoverable-collapse.js",
            "src/assets/AdminAssets/js/misc.js",
            "src/assets/AdminAssets/js/dashboard.js"

          ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "PharmaFinder:build:production"
            },
            "development": {
              "browserTarget": "PharmaFinder:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "PharmaFinder:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
